{
  "manifest_version": 2,

  "name": "NewsSearch",
  "description": "NaverNewsSearch",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "storage",
    "activeTab",
    "tabs"
  ]
}